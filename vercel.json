{
    "version" : 2,
    "builds" : [
        {
            "src" : "cars_world/wsgi.py",
            "use" : "@vercel/python",
            "config" : {
                "maxLambdaSize" : "15mb",
                "runtime" : "python-3.10.4"
            }
        },
        {
            "src" : "build.sh",
            "use" : "@vercel/static-build",
            "config" : {
                "distDir" : "staticfiles_build"
            }
        }
    ],
    "routes" : [
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "cars_world/wsgi.py"
        }
    ]

}